<div class="user_page">
  <h1>Welcome <%= @user.username %></h1>
  <form action="<%= tweets_url %>" method="POST">
    <%= render "shared/auth_token" %>
    <input
      type="text"
      name="tweet[tweet]"
      placeholder="What's happening?">
    <button>Tweet</button>
  </form>
  <ul>
    <% @all_tweets.each do |tweet| %>
      <li>
        <%= tweet.tweet %> - <%= link_to tweet.user.username, user_url(tweet.user) %></li>
      </li>
    <% end %>
  </ul>
</div>
