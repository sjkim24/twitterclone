<div class="signup">
  <h1>Sign Up</h2>
    <form action="<%= users_url %>" method="post">
      <%= render "shared/auth_token" %>
      <input id="user_username" placeholder="Username"
        type="text" name="user[username]">
      <input id="user_password" placeholder="Password"
        type="password" name="user[password]">
      <button>Sign Up</button>
    </form>
</div>
