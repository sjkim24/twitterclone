<div class="login">
  <h1>Log In</h1>
    <form action="<%= session_url %>" method="POST">
      <%= render "shared/auth_token" %>
      <input
        id="user_username"
        type="text"
        name="user[username]"
        placeholder="Username">
      <input
        id="user_password"
        type="password"
        name="user[password]"
        placeholder="Password">
      <button>Log In</button>
    </form>
</div>
